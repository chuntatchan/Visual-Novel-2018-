  m_Script: {fileID: 11500000, guid: 75a7dff2ad66a434687e07c36395ed97, type: 3}
  m_Name: 
  m_EditorClassIdentifier: 
  StoryLinesArray:
  - _storySentence:
    - storySentenceText: "It\u2019s night-time, just barely dusk. You\u2019re at your
        desk crawling through homework, the same place you\u2019ve been for the past
        three hours."
      _audioClipStart: {fileID: 0}
      _audioClipVolume: 0
      newBGImage: {fileID: 0}
      animationToPreform: 0
      charaAnimation: -1
      charaName: 
      useName: 0
      newCharaLook: {fileID: 0}
      newCharaLookInt: -1
      textBoxColor: {r: 0, g: 0, b: 0, a: 0}
      _fontStyle: 2
    - storySentenceText: "Maia\u2019s at lacrosse practice, so she hasn\u2019t been
        able to help you or text you memes of encouragement. All the words on your
        paper are blurring together. You groan and knock your head against the desk.
        This isn\u2019t the worst kind of torture ever, but it sure feels like it."
      _audioClipStart: {fileID: 0}
      _audioClipVolume: 0
      newBGImage: {fileID: 0}
      animationToPreform: 0
      charaAnimation: -1
      charaName: 
      useName: 0
      newCharaLook: {fileID: 0}
      newCharaLookInt: -1
      textBoxColor: {r: 0, g: 0, b: 0, a: 0}
      _fontStyle: 2
    - storySentenceText: Wait, did you just hear a second knock?
      _audioClipStart: {fileID: 0}
      _audioClipVolume: 0
      newBGImage: {fileID: 0}
      animationToPreform: 0
      charaAnimation: -1
      charaName: 
      useName: 0
      newCharaLook: {fileID: 0}
      newCharaLookInt: -1
      textBoxColor: {r: 0, g: 0, b: 0, a: 0}
      _fontStyle: 2
    - storySentenceText: '!! There it is again!'
      _audioClipStart: {fileID: 0}
      _audioClipVolume: 0
      newBGImage: {fileID: 0}
      animationToPreform: 0
      charaAnimation: -1
      charaName: 
      useName: 0
      newCharaLook: {fileID: 0}
      newCharaLookInt: -1
      textBoxColor: {r: 0, g: 0, b: 0, a: 0}
      _fontStyle: 2
    - storySentenceText: You look up and almost fall out of your chair when you see
        Elise perched on your windowsill knocking on your window. You frantically
        pull up the windowsill and she slides into your room.
      _audioClipStart: {fileID: 0}
      _audioClipVolume: 0
      newBGImage: {fileID: 0}
      animationToPreform: 0
      charaAnimation: -1
      charaName: 
      useName: 0
      newCharaLook: {fileID: 0}
      newCharaLookInt: -1
      textBoxColor: {r: 0, g: 0, b: 0, a: 0}
      _fontStyle: 2
    - storySentenceText: Elise!! How did you find my house?!
      _audioClipStart: {fileID: 0}
      _audioClipVolume: 0
      newBGImage: {fileID: 0}
      animationToPreform: 0
      charaAnimation: -1
      charaName: You
      useName: 1
      newCharaLook: {fileID: 0}
      newCharaLookInt: -1
      textBoxColor: {r: 0, g: 0, b: 0, a: 0}
      _fontStyle: 0
    - storySentenceText: Hey stranger! I live right next to you actually, near the
        creek down the road. It was easy to see you walking home!
      _audioClipStart: {fileID: 0}
      _audioClipVolume: 0
      newBGImage: {fileID: 0}
      animationToPreform: 0
      charaAnimation: -1
      charaName: Elise
      useName: 1
      newCharaLook: {fileID: 0}
      newCharaLookInt: -1
      textBoxColor: {r: 0, g: 0, b: 0, a: 0}
      _fontStyle: 0
    - storySentenceText: "This would probably be way creepier if it wasn\u2019t Elise
        saying it. You shrug it off and try not to be overwhelmed by the fact that
        Elise is in. Your. Room."
      _audioClipStart: {fileID: 0}
      _audioClipVolume: 0
      newBGImage: {fileID: 0}
      animationToPreform: 0
      charaAnimation: -1
      charaName: 
      useName: 0
      newCharaLook: {fileID: 0}
      newCharaLookInt: -1
      textBoxColor: {r: 0, g: 0, b: 0, a: 0}
      _fontStyle: 2
    - storySentenceText: What are you doing here?
      _audioClipStart: {fileID: 0}
      _audioClipVolume: 0
      newBGImage: {fileID: 0}
      animationToPreform: 0
      charaAnimation: -1
      charaName: You
      useName: 1
      newCharaLook: {fileID: 0}
      newCharaLookInt: -1
      textBoxColor: {r: 0, g: 0, b: 0, a: 0}
      _fontStyle: 0
    - storySentenceText: Did you forget about my promise already?
      _audioClipStart: {fileID: 0}
      _audioClipVolume: 0
      newBGImage: {fileID: 0}
      animationToPreform: 0
      charaAnimation: -1
      charaName: Elise
      useName: 1
      newCharaLook: {fileID: 0}
      newCharaLookInt: -1
      textBoxColor: {r: 0, g: 0, b: 0, a: 0}
      _fontStyle: 0
  - _storySentence:
    - storySentenceText: "I\u2019m sorry, what promise again? My brain is fried from
        schoolwork."
      _audioClipStart: {fileID: 0}
      _audioClipVolume: 0
      newBGImage: {fileID: 0}
      animationToPreform: 0
      charaAnimation: -1
      charaName: You
      useName: 1
      newCharaLook: {fileID: 0}
      newCharaLookInt: -1
      textBoxColor: {r: 0, g: 0, b: 0, a: 0}
      _fontStyle: 0
    - storySentenceText: "The concert, silly! It\u2019s tonight, and I\u2019m here
        to whisk you away!"
      _audioClipStart: {fileID: 0}
      _audioClipVolume: 0
      newBGImage: {fileID: 0}
      animationToPreform: 0
      charaAnimation: -1
      charaName: Elise
      useName: 1
      newCharaLook: {fileID: 0}
      newCharaLookInt: -1
      textBoxColor: {r: 0, g: 0, b: 0, a: 0}
      _fontStyle: 0
    - storySentenceText: "That\u2019s right! You totally forgot about it!!"
      _audioClipStart: {fileID: 0}
      _audioClipVolume: 0
      newBGImage: {fileID: 0}
      animationToPreform: 0
      charaAnimation: -1
      charaName: 
      useName: 0
      newCharaLook: {fileID: 0}
      newCharaLookInt: -1
      textBoxColor: {r: 0, g: 0, b: 0, a: 0}
      _fontStyle: 2
  - _storySentence:
    - storySentenceText: Oh, right! The concert! I was so wrapped up in work, I almost
        forgot!
      _audioClipStart: {fileID: 0}
      _audioClipVolume: 0
      newBGImage: {fileID: 0}
      animationToPreform: 0
      charaAnimation: -1
      charaName: You
      useName: 1
      newCharaLook: {fileID: 0}
      newCharaLookInt: -1
      textBoxColor: {r: 0, g: 0, b: 0, a: 0}
      _fontStyle: 0
    - storySentenceText: "That\u2019s right! It\u2019s tonight, and I have to take
        you. It\u2019s my favorite band!"
      _audioClipStart: {fileID: 0}
      _audioClipVolume: 0
      newBGImage: {fileID: 0}
      animationToPreform: 0
      charaAnimation: -1
      charaName: Elise
      useName: 1
      newCharaLook: {fileID: 0}
      newCharaLookInt: -1
      textBoxColor: {r: 0, g: 0, b: 0, a: 0}
      _fontStyle: 0
  - _storySentence:
    - storySentenceText: Elise does a little spin and plops down onto your bed. How
        is she so cute all the time?
      _audioClipStart: {fileID: 0}
      _audioClipVolume: 0
      newBGImage: {fileID: 0}
      animationToPreform: 0
      charaAnimation: -1
      charaName: 
      useName: 0
      newCharaLook: {fileID: 0}
      newCharaLookInt: -1
      textBoxColor: {r: 0, g: 0, b: 0, a: 0}
      _fontStyle: 2
    - storySentenceText: "Give me a second to grab some warmer clothes and I\u2019ll
        be ready!"
      _audioClipStart: {fileID: 0}
      _audioClipVolume: 0
      newBGImage: {fileID: 0}
      animationToPreform: 0
      charaAnimation: -1
      charaName: You
      useName: 1
      newCharaLook: {fileID: 0}
      newCharaLookInt: -1
      textBoxColor: {r: 0, g: 0, b: 0, a: 0}
      _fontStyle: 0
    - storySentenceText: "You\u2019re in the middle of pulling on your jacket when
        you remember the pile of work on your desk. You turn and your heart sinks."
      _audioClipStart: {fileID: 0}
      _audioClipVolume: 0
      newBGImage: {fileID: 0}
      animationToPreform: 0
      charaAnimation: -1
      charaName: 
      useName: 0
      newCharaLook: {fileID: 0}
      newCharaLookInt: -1
      textBoxColor: {r: 0, g: 0, b: 0, a: 0}
      _fontStyle: 2
    - storySentenceText: "Fuck, I forgot how much work I still need to do by tomorrow\u2026"
      _audioClipStart: {fileID: 0}
      _audioClipVolume: 0
      newBGImage: {fileID: 0}
      animationToPreform: 0
      charaAnimation: -1
      charaName: You
      useName: 1
      newCharaLook: {fileID: 0}
      newCharaLookInt: -1
      textBoxColor: {r: 0, g: 0, b: 0, a: 0}
      _fontStyle: 0
    - storySentenceText: Let me take a look at it.
      _audioClipStart: {fileID: 0}
      _audioClipVolume: 0
      newBGImage: {fileID: 0}
      animationToPreform: 0
      charaAnimation: -1
      charaName: Elise
      useName: 1
      newCharaLook: {fileID: 0}
      newCharaLookInt: -1
      textBoxColor: {r: 0, g: 0, b: 0, a: 0}
      _fontStyle: 0
    - storySentenceText: Elise rifles through the pile of work in less than a minute.
        She stacks it back up and tosses it aside.
      _audioClipStart: {fileID: 0}
      _audioClipVolume: 0
      newBGImage: {fileID: 0}
      animationToPreform: 0
      charaAnimation: -1
      charaName: 
      useName: 0
      newCharaLook: {fileID: 0}
      newCharaLookInt: -1
      textBoxColor: {r: 0, g: 0, b: 0, a: 0}
      _fontStyle: 2
    - storySentenceText: "All of this is pretty easy. I\u2019ll do it for you later
        tonight if we leave right now. I want to get a spot at the front!"
      _audioClipStart: {fileID: 0}
      _audioClipVolume: 0
      newBGImage: {fileID: 0}
      animationToPreform: 0
      charaAnimation: -1
      charaName: Elise
      useName: 1
      newCharaLook: {fileID: 0}
      newCharaLookInt: -1
      textBoxColor: {r: 0, g: 0, b: 0, a: 0}
      _fontStyle: 0
    - storySentenceText: "Now this is an offer you can\u2019t refuse."
      _audioClipStart: {fileID: 0}
      _audioClipVolume: 0
      newBGImage: {fileID: 0}
      animationToPreform: 0
      charaAnimation: -1
      charaName: 
      useName: 0
      newCharaLook: {fileID: 0}
      newCharaLookInt: -1
      textBoxColor: {r: 0, g: 0, b: 0, a: 0}
      _fontStyle: 2
    - storySentenceText: "I\u2019m in!! But we\u2019re gonna have to sneak out my
        window."
      _audioClipStart: {fileID: 0}
      _audioClipVolume: 0
      newBGImage: {fileID: 0}
      animationToPreform: 0
      charaAnimation: -1
      charaName: You
      useName: 1
      newCharaLook: {fileID: 0}
      newCharaLookInt: -1
      textBoxColor: {r: 0, g: 0, b: 0, a: 0}
      _fontStyle: 0
    - storySentenceText: "It\u2019s dark, but barely. Countless stars light up the
        sky on a beautiful cloudless night. Elise helps you down from the window,
        and takes it all in, eyes sparkling."
      _audioClipStart: {fileID: 0}
      _audioClipVolume: 0
      newBGImage: {fileID: 21300000, guid: 516a6879131a940158018e9feda99dc5, type: 3}
      animationToPreform: 0
      charaAnimation: -1
      charaName: 
      useName: 0
      newCharaLook: {fileID: 0}
      newCharaLookInt: -1
      textBoxColor: {r: 0, g: 0, b: 0, a: 0}
      _fontStyle: 2
    - storySentenceText: "Isn\u2019t it amazing? I love going for a run on nights
        like this."
      _audioClipStart: {fileID: 0}
      _audioClipVolume: 0
      newBGImage: {fileID: 0}
      animationToPreform: 0
      charaAnimation: -1
      charaName: Elise
      useName: 1
      newCharaLook: {fileID: 0}
      newCharaLookInt: -1
      textBoxColor: {r: 0, g: 0, b: 0, a: 0}
      _fontStyle: 0
    - storySentenceText: It really is. You run at night?
      _audioClipStart: {fileID: 0}
      _audioClipVolume: 0
      newBGImage: {fileID: 0}
      animationToPreform: 0
      charaAnimation: -1
      charaName: You
      useName: 1
      newCharaLook: {fileID: 0}
      newCharaLookInt: -1
      textBoxColor: {r: 0, g: 0, b: 0, a: 0}
      _fontStyle: 0
    - storySentenceText: "Isn\u2019t it clear yet that I do as I please?"
      _audioClipStart: {fileID: 0}
      _audioClipVolume: 0
      newBGImage: {fileID: 0}
      animationToPreform: 0
      charaAnimation: -1
      charaName: Elise
      useName: 1
      newCharaLook: {fileID: 0}
      newCharaLookInt: -1
      textBoxColor: {r: 0, g: 0, b: 0, a: 0}
      _fontStyle: 0
    - storySentenceText: Fair enough. So, where is the concert?
      _audioClipStart: {fileID: 0}
      _audioClipVolume: 0
      newBGImage: {fileID: 0}
      animationToPreform: 0
      charaAnimation: -1
      charaName: You
      useName: 1
      newCharaLook: {fileID: 0}
      newCharaLookInt: -1
      textBoxColor: {r: 0, g: 0, b: 0, a: 0}
      _fontStyle: 0
    - storySentenceText: Just one town over! It's only about ten miles from here actually.
      _audioClipStart: {fileID: 0}
      _audioClipVolume: 0
      newBGImage: {fileID: 0}
      animationToPreform: 0
      charaAnimation: -1
      charaName: Elise
      useName: 1
      newCharaLook: {fileID: 0}
      newCharaLookInt: -1
      textBoxColor: {r: 0, g: 0, b: 0, a: 0}
      _fontStyle: 0
    - storySentenceText: "Wait, ten miles?! How are we going to get there?? I don\u2019t
        know how to drive!!!"
      _audioClipStart: {fileID: 0}
      _audioClipVolume: 0
      newBGImage: {fileID: 0}
      animationToPreform: 0
      charaAnimation: -1
      charaName: You
      useName: 1
      newCharaLook: {fileID: 0}
      newCharaLookInt: -1
      textBoxColor: {r: 0, g: 0, b: 0, a: 0}
      _fontStyle: 0
    - storySentenceText: "You start picking at your nails, trying to quell the rising
        feeling of panic in your stomach. Elise was never one for planning. What if
        everything\u2019s ruined because neither of you thought of transportation?"
      _audioClipStart: {fileID: 0}
      _audioClipVolume: 0
      newBGImage: {fileID: 0}
      animationToPreform: 0
      charaAnimation: -1
      charaName: 
      useName: 0
      newCharaLook: {fileID: 0}
      newCharaLookInt: -1
      textBoxColor: {r: 0, g: 0, b: 0, a: 0}
      _fontStyle: 2
    - storySentenceText: Elise takes one of your hands and puts it on her shoulder.
      _audioClipStart: {fileID: 0}
      _audioClipVolume: 0
      newBGImage: {fileID: 0}
      animationToPreform: 0
      charaAnimation: -1
      charaName: 
      useName: 0
      newCharaLook: {fileID: 0}
      newCharaLookInt: -1
      textBoxColor: {r: 0, g: 0, b: 0, a: 0}
      _fontStyle: 2
    - storySentenceText: "Don\u2019t worry. I have an idea. Close your eyes for me?
        And don\u2019t move your hand."
      _audioClipStart: {fileID: 0}
      _audioClipVolume: 0
      newBGImage: {fileID: 0}
      animationToPreform: 0
      charaAnimation: -1
      charaName: Elise
      useName: 1
      newCharaLook: {fileID: 0}
      newCharaLookInt: -1
      textBoxColor: {r: 0, g: 0, b: 0, a: 0}
      _fontStyle: 0
    - storySentenceText: You do as she asks. If she was going to murder you, she probably
        would have done it by now.
      _audioClipStart: {fileID: 0}
      _audioClipVolume: 0
      newBGImage: {fileID: 0}
      animationToPreform: 0
      charaAnimation: -1
      charaName: 
      useName: 0
      newCharaLook: {fileID: 0}
      newCharaLookInt: -1
      textBoxColor: {r: 0, g: 0, b: 0, a: 0}
      _fontStyle: 2
    - storySentenceText: "Elise\u2019s shoulder is slight beneath your palm, but as
        seconds pass something about it...changes. The texture of her shirt feels
        more coarse. It\u2019s a cold night, but somehow she\u2019s gotten warmer
        too."
      _audioClipStart: {fileID: 0}
      _audioClipVolume: 0
      newBGImage: {fileID: 0}
      animationToPreform: 0
      charaAnimation: -1
      charaName: 
      useName: 0
      newCharaLook: {fileID: 0}
      newCharaLookInt: -1
      textBoxColor: {r: 0, g: 0, b: 0, a: 0}
      _fontStyle: 2
    - storySentenceText: You hear a deep huff and open your eyes.
      _audioClipStart: {fileID: 0}
      _audioClipVolume: 0
      newBGImage: {fileID: 0}
      animationToPreform: 0
      charaAnimation: -1
      charaName: 
      useName: 0
      newCharaLook: {fileID: 0}
      newCharaLookInt: -1
      textBoxColor: {r: 0, g: 0, b: 0, a: 0}
      _fontStyle: 2
    - storySentenceText: Elise is gone. In her place is a massive, jet-black horse
        with what looks like seaweed tangled into its wild mane, your hand one one
        of its massive flanks.
      _audioClipStart: {fileID: 0}
      _audioClipVolume: 0
      newBGImage: {fileID: 0}
      animationToPreform: 0
      charaAnimation: -1
      charaName: 
      useName: 0
      newCharaLook: {fileID: 0}
      newCharaLookInt: -1
      textBoxColor: {r: 0, g: 0, b: 0, a: 0}
      _fontStyle: 2
    - storySentenceText: "You stumble backwards and fall gracelessly on your butt.
        The horse snuffles a little, almost like it\u2019s laughing at you."
      _audioClipStart: {fileID: 0}
      _audioClipVolume: 0
      newBGImage: {fileID: 0}
      animationToPreform: 0
      charaAnimation: -1
      charaName: 
      useName: 0
      newCharaLook: {fileID: 0}
      newCharaLookInt: -1
      textBoxColor: {r: 0, g: 0, b: 0, a: 0}
      _fontStyle: 2
    - storySentenceText: "Holy\u2014"
      _audioClipStart: {fileID: 0}
      _audioClipVolume: 0
      newBGImage: {fileID: 0}
      animationToPreform: 0
      charaAnimation: -1
      charaName: You
      useName: 1
      newCharaLook: {fileID: 0}
      newCharaLookInt: -1
      textBoxColor: {r: 0, g: 0, b: 0, a: 0}
      _fontStyle: 0
    - storySentenceText: "Don\u2019t be nervous!"
      _audioClipStart: {fileID: 0}
      _audioClipVolume: 0
      newBGImage: {fileID: 0}
      animationToPreform: 0
      charaAnimation: -1
      charaName: Elise
      useName: 1
      newCharaLook: {fileID: 0}
      newCharaLookInt: -1
      textBoxColor: {r: 0, g: 0, b: 0, a: 0}
      _fontStyle: 0
    - storySentenceText: What? Elise? Where are you??
      _audioClipStart: {fileID: 0}
      _audioClipVolume: 0
      newBGImage: {fileID: 0}
      animationToPreform: 0
      charaAnimation: -1
      charaName: You
      useName: 1
      newCharaLook: {fileID: 0}
      newCharaLookInt: -1
      textBoxColor: {r: 0, g: 0, b: 0, a: 0}
      _fontStyle: 0
    - storySentenceText: "You can hear her voice clear as day, but it sounds different
        somehow. Like she\u2019s talking to you over a phone with a perfect connection."
      _audioClipStart: {fileID: 0}
      _audioClipVolume: 0
      newBGImage: {fileID: 0}
      animationToPreform: 0
      charaAnimation: -1
      charaName: 
      useName: 0
      newCharaLook: {fileID: 0}
      newCharaLookInt: -1
      textBoxColor: {r: 0, g: 0, b: 0, a: 0}
      _fontStyle: 2
    - storySentenceText: You never took your hand off of my shoulder, right?
      _audioClipStart: {fileID: 0}
      _audioClipVolume: 0
      newBGImage: {fileID: 0}
      animationToPreform: 0
      charaAnimation: -1
      charaName: Elise
      useName: 1
      newCharaLook: {fileID: 0}
      newCharaLookInt: -1
      textBoxColor: {r: 0, g: 0, b: 0, a: 0}
      _fontStyle: 0
    - storySentenceText: "\u2026.I suppose not..."
      _audioClipStart: {fileID: 0}
      _audioClipVolume: 0
      newBGImage: {fileID: 0}
      animationToPreform: 0
      charaAnimation: -1
      charaName: You
      useName: 1
      newCharaLook: {fileID: 0}
      newCharaLookInt: -1
      textBoxColor: {r: 0, g: 0, b: 0, a: 0}
      _fontStyle: 0
    - storySentenceText: Does that help answer the question?
      _audioClipStart: {fileID: 0}
      _audioClipVolume: 0
      newBGImage: {fileID: 0}
      animationToPreform: 0
      charaAnimation: -1
      charaName: Elise
      useName: 1
      newCharaLook: {fileID: 0}
      newCharaLookInt: -1
      textBoxColor: {r: 0, g: 0, b: 0, a: 0}
      _fontStyle: 0
    - storySentenceText: '...'
      _audioClipStart: {fileID: 0}
      _audioClipVolume: 0
      newBGImage: {fileID: 0}
      animationToPreform: 0
      charaAnimation: -1
      charaName: You
      useName: 1
      newCharaLook: {fileID: 0}
      newCharaLookInt: -1
      textBoxColor: {r: 0, g: 0, b: 0, a: 0}
      _fontStyle: 0
    - storySentenceText: "It\u2019s okay, <pname>, this is always the part that surprises
        humans. I figured I\u2019d get it over with quickly."
      _audioClipStart: {fileID: 0}
      _audioClipVolume: 0
      newBGImage: {fileID: 0}
